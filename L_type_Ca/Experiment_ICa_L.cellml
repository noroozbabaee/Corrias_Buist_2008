<?xml version='1.0' encoding='UTF-8'?>
<model name="experiment_LCa" xmlns="http://www.cellml.org/cellml/1.1#" xmlns:cellml="http://www.cellml.org/cellml/1.1#" xmlns:xlink="http://www.w3.org/1999/xlink">
    <import xlink:href="../common/units.cellml">
        <units name="time_units" units_ref="time_units"/>
        <units name="millimolar" units_ref="millimolar"/>
        <units name="R_units" units_ref="R_units"/>
        <units name="F_units" units_ref="F_units"/>
        <units name="Temperature_units" units_ref="Temperature_units"/>
        <units name="conductance_units" units_ref="conductance_units"/>
        <units name="Inverse_Voltage_units" units_ref="Inverse_Voltage_units"/>
        <units name="voltage_units" units_ref="voltage_units"/>
        <units name="current_units" units_ref="current_units"/>
    </import>
    <import xlink:href="../common/generator.cellml">
        <component component_ref="experiment" name="experiment"/>
    </import>
    <connection>
        <map_components component_1="experiment" component_2="I_Ltype"/>
        <map_variables variable_1="time" variable_2="time"/>
        <map_variables variable_1="Ca_i" variable_2="Ca_i"/>
        <map_variables variable_1="Vm" variable_2="Vm"/>
    </connection>
    <import xlink:href="ICa_L.cellml">
        <component component_ref="I_Ltype" name="I_Ltype"/>
    </import>
    <import xlink:href="../common/parameters.cellml">
        <component component_ref="parameter_values" name="parameter_values"/>
    </import>
    <connection>
        <map_components component_1="parameter_values" component_2="I_Ltype"/>
        <map_variables variable_1="RToF" variable_2="RToF"/>
        <map_variables variable_1="Ca_o" variable_2="Ca_o"/>
        <map_variables variable_1="T_correction_Ca" variable_2="T_correction_Ca"/>
    </connection>
    <component name="output">
        <variable name="time" public_interface="out" units="time_units"/>
        <variable name="Vm" public_interface="in" units="voltage_units"/>
        <variable name="E_Ca" public_interface="in" units="voltage_units"/>
        <variable name="I_Ltype" public_interface="in" units="current_units"/>
        <variable name="d_Ltype" public_interface="in" units="dimensionless"/>
        <variable name="f_Ltype" public_interface="in" units="dimensionless"/>
    </component>
    <connection>
        <map_components component_1="output" component_2="I_Ltype"/>
        <map_variables variable_1="time" variable_2="time"/>
        <map_variables variable_1="E_Ca" variable_2="E_Ca"/>
        <map_variables variable_1="Vm" variable_2="Vm"/>
        <map_variables variable_1="I_Ltype" variable_2="I_Ltype"/>
        <map_variables variable_1="d_Ltype" variable_2="d_Ltype"/>
        <map_variables variable_1="f_Ltype" variable_2="f_Ltype"/>
    </connection>
</model>
